(deftemplate tabla
	(slot l1c1)
	(slot l1c2)
	(slot l1c3)
	(slot l2c1)
	(slot l2c2)
	(slot l2c3)
	(slot l3c1)
	(slot l3c2)
	(slot l3c3)
)

(deffacts incepere
	(incepeti jocul?)
)

(defrule start
	?a<-(incepeti jocul?)
	=>
	(retract ?a)
	(printout t "Salutare! Acesta este un joc X/0 in care" crlf "dumneavoastra incepeti primul si sunteti cu X." crlf "Mult succes!" crlf "Doriti sa incepeti?" crlf)
	(printout t "Da / Nu ?" crlf)
	(printout t "Raspunsul dvs: ")
	(bind ?x (read))
	(assert (speranta ?x))
)

(defrule continuare1 (declare(salience 9))
	?a<-(speranta nu)
	=>
	(retract ?a)
	(printout t "Pa Pa!" crlf)
	(halt)
)

(defrule stop
	?a<-(speranta ?s)
	(not(test (eq ?s nu)))
	(not(test (eq ?s da)))
	=>
	(retract ?a)
	(printout t "Raspuns gresit, Pa Pa!" crlf)
	(halt)
)

(defrule raspunsDa
	?a<-(speranta da)
	=>
	(retract ?a)
	(assert (tabla (l1c1 -)(l1c2 -)(l1c3 -)(l2c1 -)(l2c2 -)(l2c3 -)(l3c1 -)(l3c2 -)(l3c3 -)))
	(assert (prima mutare))
)

(defrule continuare2 (declare(salience 8))
	(ceva da)
	?a<-(tabla (l1c1 ?q)(l1c2 ?w)(l1c3 ?e)(l2c1 ?r)(l2c2 ?t)(l2c3 ?y)(l3c1 ?u)(l3c2 ?i)(l3c3 ?o))
	;(not(IA))
	=>
	(printout t "Aceasta este tabla de joc:" crlf ?q " " ?w " " ?e crlf ?r " " ?t " " ?y crlf ?u " " ?i " " ?o crlf)
	(printout t "Spuneti LINIA unde vreti sa puneti X: ")
	(bind ?n (read))
	(printout t "Spuneti COLOANA unde vreti sa puneti X: ")
	(bind ?m (read))
	(assert (l ?n c ?m))
	(printout t crlf)
)

(defrule primaMutare2 (declare(salience 8))
	?b<-(prima mutare)
	?a<-(tabla (l1c1 ?q)(l1c2 ?w)(l1c3 ?e)(l2c1 ?r)(l2c2 ?t)(l2c3 ?y)(l3c1 ?u)(l3c2 ?i)(l3c3 ?o))
	=>
	(printout t "Aceasta este tabla de joc:" crlf ?q " " ?w " " ?e crlf ?r " " ?t " " ?y crlf ?u " " ?i " " ?o crlf)
	(printout t "Spuneti LINIA unde vreti sa puneti X: ")
	(bind ?n (read))
	(printout t "Spuneti COLOANA unde vreti sa puneti X: ")
	(bind ?m (read))
	(assert (l ?n c ?m))
	(printout t crlf)
	(retract ?b)
	(assert (prima mutare2))
)

