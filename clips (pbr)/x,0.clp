	(printout t "Ai pierdut!" crlf "M-ai jucati o runda? Da / Nu" crlf "Raspunsul dvs: ")
	(bind ?x (read))
	(assert (joaca? ?x))
)

(defrule playAgain
	?a<-(joaca? da)
	?b<-(tabla (l1c1 ?)(l1c2 ?)(l1c3 ?)(l2c1 ?)(l2c2 ?)(l2c3 ?)(l3c1 ?)(l3c2 ?)(l3c3 ?))
	=>
	(retract ?a)
	(retract ?b)
	(assert (incepeti jocul?))
)

(defrule playAgainNot (declare(salience 9))
	?a<-(joaca? nu)
	?b<-(tabla (l1c1 ?)(l1c2 ?)(l1c3 ?)(l2c1 ?)(l2c2 ?)(l2c3 ?)(l3c1 ?)(l3c2 ?)(l3c3 ?))
	=>
	(retract ?b)
	(retract ?a)
	(assert (speranta nu))
)

(defrule remiza(declare(salience 9))
	(tabla (l1c1 ?q)(l1c2 ?w)(l1c3 ?e)(l2c1 ?r)(l2c2 ?t)(l2c3 ?y)(l3c1 ?u)(l3c2 ?i)(l3c3 ?o))
	(not(test(eq ?q -)))
	(not(test(eq ?w -)))
	(not(test(eq ?e -)))
	(not(test(eq ?r -)))
	(not(test(eq ?t -)))
	(not(test(eq ?y -)))
	(not(test(eq ?u -)))
	(not(test(eq ?i -)))
	(not(test(eq ?o -)))
	=>
	(printout t "Remiza!" crlf "Mai jucati o runda? Da / Nu" crlf "Raspunsul dvs: ")
	(bind ?x (read))
	(assert (joaca? ?x))
)
 
