(defrule playerWins(declare(salience 9))
	?c<-(ceva da)
	(tabla (l1c1 ?q)(l1c2 ?w)(l1c3 ?e)(l2c1 ?r)(l2c2 ?t)(l2c3 ?y)(l3c1 ?u)(l3c2 ?i)(l3c3 ?o))
	(or(tabla (l1c1 X)(l1c2 X)(l1c3 X)(l2c1 ?)(l2c2 ?)(l2c3 ?)(l3c1 ?)(l3c2 ?)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 ?)(l2c1 X)(l2c2 X)(l2c3 X)(l3c1 ?)(l3c2 ?)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 ?)(l2c1 ?)(l2c2 ?)(l2c3 ?)(l3c1 X)(l3c2 X)(l3c3 X))
	   (tabla (l1c1 X)(l1c2 ?)(l1c3 ?)(l2c1 X)(l2c2 ?)(l2c3 ?)(l3c1 X)(l3c2 ?)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 X)(l1c3 ?)(l2c1 ?)(l2c2 X)(l2c3 ?)(l3c1 ?)(l3c2 X)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 X)(l2c1 ?)(l2c2 ?)(l2c3 X)(l3c1 ?)(l3c2 ?)(l3c3 X))
	   (tabla (l1c1 X)(l1c2 ?)(l1c3 ?)(l2c1 ?)(l2c2 X)(l2c3 ?)(l3c1 ?)(l3c2 ?)(l3c3 X))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 X)(l2c1 ?)(l2c2 X)(l2c3 ?)(l3c1 X)(l3c2 ?)(l3c3 ?))
	)
	=>
	(retract ?c)
	(printout t "Aceasta este tabla de joc:" crlf ?q " " ?w " " ?e crlf ?r " " ?t " " ?y crlf ?u " " ?i " " ?o crlf)
	(printout t "Felicitari, ati castigat!" crlf "M-ai jucati o runda? Da / Nu" crlf "Raspunsul dvs: ")
	(bind ?x (read))
	(assert (joaca? ?x))
)

(defrule IAWins(declare(salience 9))
	?c<-(ceva da)
	(tabla (l1c1 ?q)(l1c2 ?w)(l1c3 ?e)(l2c1 ?r)(l2c2 ?t)(l2c3 ?y)(l3c1 ?u)(l3c2 ?i)(l3c3 ?o))
	(or(tabla (l1c1 O)(l1c2 O)(l1c3 O)(l2c1 ?)(l2c2 ?)(l2c3 ?)(l3c1 ?)(l3c2 ?)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 ?)(l2c1 O)(l2c2 O)(l2c3 O)(l3c1 ?)(l3c2 ?)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 ?)(l2c1 ?)(l2c2 ?)(l2c3 ?)(l3c1 O)(l3c2 O)(l3c3 O))
	   (tabla (l1c1 O)(l1c2 ?)(l1c3 ?)(l2c1 O)(l2c2 ?)(l2c3 ?)(l3c1 O)(l3c2 ?)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 O)(l1c3 ?)(l2c1 ?)(l2c2 O)(l2c3 ?)(l3c1 ?)(l3c2 O)(l3c3 ?))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 O)(l2c1 ?)(l2c2 ?)(l2c3 O)(l3c1 ?)(l3c2 ?)(l3c3 O))
	   (tabla (l1c1 O)(l1c2 ?)(l1c3 ?)(l2c1 ?)(l2c2 O)(l2c3 ?)(l3c1 ?)(l3c2 ?)(l3c3 O))
	   (tabla (l1c1 ?)(l1c2 ?)(l1c3 O)(l2c1 ?)(l2c2 O)(l2c3 ?)(l3c1 O)(l3c2 ?)(l3c3 ?))
	)
	=>
	(retract ?c)
